{
  "buildCommand": "cd poker-player-manager && npm run build",
  "outputDirectory": "poker-player-manager/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://poker-night-app-production-985f.up.railway.app/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
